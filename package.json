{
  "name": "ts-graphql-client",
  "version": "1.1.7",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "license": "MIT",
  "description": "Typed client GraphQL using Apollo",
  "author": "RawZ06",
  "repository": "RawZ06/ts-graphql-client",
  "keywords": [
    "graphql",
    "client",
    "typescript",
    "typed",
    "apollo",
    "query",
    "mutation",
    "graphql-client",
    "client-ts",
    "ts"
  ],
  "engines": {
    "node": ">= 18.0.0"
  },
  "scripts": {
    "deploy": "tsc && npm publish",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "codegen": "graphql-codegen"
  },
  "files": [
    "dist/src/**/*.js",
    "dist/src/**/*.d.ts",
    "README.md"
  ],
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.2.4",
    "@types/jest": "^28.1.3",
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.1.0",
    "jest": "^28.1.1",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "apollo-boost": "^0.4.9",
    "graphql": "^15.6.1"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)+(spec).ts"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
